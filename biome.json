{
	"$schema": "https://biomejs.dev/schemas/1.6.0/schema.json",
	"files": {
		"include": [
			"src/**/*.ts"
		]
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"ignore": [],
		"include": [
			"src/**/*.ts"
		],
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineEnding": "lf",
		"lineWidth": 80
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		}
	}
}